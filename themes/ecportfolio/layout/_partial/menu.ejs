<!-- nav section -->
<div class="navSection">
	<!-- nav button -->
	<button id="menuButton">
		<h2 onclick="openNav()" style="display: table-cell;">&#9776 Menu</h2>
	</button>
	<!-- nav -->
	<div id="myNav" class="overlayNav">
		<a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
		<div class="overlay-content">
			<a class="nav-link  " href="#">Home</a>
			<a class="nav-link" href="#">About</a>
			<!-- add database maybe (if projects in database add in nav bar this section) -->
			<button class="accordion">
				<a class="nav-link " href="#">Projects</a></button>
			<div class="panel">
				<!-- all the projects -->
				<% site.posts.forEach(function(post){ %>
				<a class="dropdown-item" href="<%- post.path %>">
					<%- post.title %></a>
				<br />
				<% }) %>
			</div>
			<a class="nav-link" href="#">Contact</a>

		</div>
	</div>
</div>